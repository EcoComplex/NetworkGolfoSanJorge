@article{Crespi-Abril2011a,
  title = {Diet of Spring and Summer Spawning Groups of {{Illex}} Argentinus Inhabiting Coastal Waters in {{San Matias Gulf}} (Northern {{Patagonia}}, {{Argentina}})},
  author = {{Crespi-Abril}, Augusto C{\'e}sar and Trivellini, Maria Magdalena},
  year = {2011},
  month = dec,
  journal = {Aquatic Biology},
  volume = {14},
