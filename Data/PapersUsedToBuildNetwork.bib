@article{LopezCazorla2011,
  ids = {Cazorla2011},
  title = {Diet and Food Selection by {{Ramnogaster}} Arcuata ({{Osteichthyes}}, {{Clupeidae}})},
  author = {Lopez Cazorla, A. and Pettigrosso, R. E. and Tejera, L. and Camina, R.},
  year = {2011},
  journal = {Journal of Fish Biology},
  volume = {78},
  number = {7},
  pages = {2052--2066},
  issn = {1095-8649},
  doi = {10.1111/j.1095-8649.2011.02995.x},
  abstract = {The goals of this study were to describe the diet and estimate the trophic level and food selection of Jenyns's sprat Ramnogaster arcuata, one of the common fishes in the Bah\'ia Blanca estuary in Argentina. The copepods Acartia tonsa [53{$\cdot$}7\% index of relative importance (IRI)] and Eurytemora americana (13{$\cdot$}4\% IRI), the mysid Arthromysis magellanica (13{$\cdot$}9\%IRI) and the small shrimp Peisos petrunkevitchi (8{$\cdot$}2\% IRI) were the most important food items for this species, and its diet exhibited monthly variability with respect to the principal prey categories. The zooplankton species in the study area consisted of 32 taxa. Acartia tonsa was the most abundant species [54\% individuals (N) m-3], followed by Balanus glandula (13{$\cdot$}8\% N m-3) and E. americana (10{$\cdot$}1\% N m-3). The rest of the zooplankton groups represented {$<$}3\% N m-3. The most preferred items throughout the year were A. magellanica and Idotea sp. The trophic level value estimated was 3{$\cdot$}1.},
  langid = {english},
  keywords = {Argentina,Bahía Blanca,Clupeiformes,prey selectivity,south-western Atlantic Ocean,trophic level},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1095-8649.2011.02995.x},
  file = {/home/leonardo/Zotero/storage/2YTQBBJT/Cazorla et al. - 2011 - Diet and food selection by Ramnogaster arcuata (Os.pdf;/home/leonardo/Zotero/storage/42DDRVL7/Lopez Cazorla et al. - 2011 - Diet and food selection by Ramnogaster arcuata (Os.pdf;/home/leonardo/Zotero/storage/57PVJNW9/j.1095-8649.2011.02995.html}
