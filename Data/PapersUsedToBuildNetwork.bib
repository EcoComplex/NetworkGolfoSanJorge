  file = {/home/leonardo/Zotero/storage/FJP7V7ZP/Sánchez and Prenski - 1996 - Ecología trófica de peces demersales en el Golfo S.pdf}
}

@article{Sanchez2009a,
  title = {{Alimentaci\'on de la merluza (Merluccius hubbsi) en el Golfo San Jorge y aguas adyacentes}},
  author = {S{\'a}nchez, F.},
  year = {2009},
  publisher = {{Mar del Plata: Instituto Nacional de Investigaci\'on y Desarrollo Pesquero}},
  issn = {0327-9642},
  abstract = {The information obtained in summer research cruises carried out in 1995, 1999 and 2002 allowed to determine the trophic spectrum of common hake (Merluccius hubbsi) and to estimate anchovy (Engraulis anchoita) and hake consumption in the nursery area of the San Jorge Gulf and shelf adjacent waters (Argentine Sea, SW Atlantic). The area surveyed extended from 44\textordmasculine S to 47\textordmasculine S and between the 50-100 m isobaths. In the stomach content the most frequent items were crustaceans followed by fish. In all size groups sampled the presence of anchovy and common hake was observed. In the area assessed consumption of anchovy, weighted for the three summer months of 1995 was 56,639 t. The greatest predation pressure was observed on classes between 14-16 cm TL. Hake consumption estimated at 16,797 t in summer 1995 reached 36,456 t in 2002. The average length of common hake preys was 16 cm TL in 1995 and 13 cm TL in 2002.},
