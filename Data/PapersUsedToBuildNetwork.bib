@article{Belleggia2008a,
  title = {Food Habits of the Broad Nose Skate, {{Bathyraja}} Brachyurops ({{Chondrichthyes}}, {{Rajidae}}), in the South-West {{Atlantic}}},
  author = {Belleggia, Mauro and Mabraga{\~n}a, Ezequiel and Figueroa, Daniel E. and Scenna, Lorena B. and Barbini, Santiago A. and de Astarloa, Juan M. D{\'i}az},
  year = {2008},
  month = dec,
  journal = {Scientia Marina},
  volume = {72},
  number = {4},
  pages = {701--710},
  issn = {1886-8134},
  doi = {10.3989/scimar.2008.72n4701},
  abstract = {Food habits of Bathyraja brachyurops were studied based on stomach content analyses of 346 specimens collected from research cruises carried out from 2003 to 2005 on the Argentinean continental shelf (36\textordmasculine S-55\textordmasculine S). A total of 265 stomachs (76.6\%) contained food, and thirty-five taxonomic levels of prey were identified. The most important prey were fishes followed by isopods. Trophic level analysis revealed that B. brachyurops is a tertiary consumer throughout its life history. There were no differences between sexes and regions in the diet composition, but dietary shifts with ontogeny were found. The Levins' standardized index indicated wider niche breadth for small skates, whereas larger skate specimens showed a narrow niche breadth with a specialization in fishes.},
  copyright = {Copyright (c) 2008 Consejo Superior de Investigaciones Cient\'ificas (CSIC)},
  langid = {english},
  keywords = {Argentina,Bathyraja,broad nose skate,ecología tróﬁca,elasmobranchs,elasmobranquios,raya de cola corta,trophic ecology},
  file = {/home/leonardo/Zotero/storage/UIWKWPIN/Belleggia et al. - 2008 - Food habits of the broad nose skate, Bathyraja bra.pdf;/home/leonardo/Zotero/storage/H6W2XSG4/1028.html}
}

@article{Belleggia2012d,
  title = {Long-Term Changes in the Spiny Dogfish ({{Squalus}} Acanthias) Trophic Role in the Southwestern {{Atlantic}}},
  author = {Belleggia, Mauro and Figueroa, Daniel Enrique and S{\'a}nchez, Felisa and Bremec, Claudia},
  year = {2012},
  month = mar,
  journal = {Hydrobiologia},
  volume = {684},
  number = {1},
  pages = {57--67},
  issn = {1573-5117},
  doi = {10.1007/s10750-011-0967-y},
  abstract = {We used a comprehensive dataset on the diet of Squalus acanthias in the southwestern Atlantic Ocean (35\textendash 55\textdegree S) to test the hypothesis that fishery has caused a decrease in the trophic level of this species (fishing down process). Of 3,644 stomachs examined between the years 1985 and 2010, 2,214 (60.77\%) contained prey items. Generalized linear models were used to evaluate the support in our data for five explanatory variables (sex, predator's total length, season, region and decade) that may explain the consumption of the given prey. The frequency of fish, mainly Merluccius hubbsi, and benthos in the stomachs decreased through time, whereas the squid Illex argentinus and jellyfishes exhibited positive trends. The trophic level of S. acanthias declined from the 1980s to the 1990s due to the high consumption of prey items at lower trophic levels. On the other hand, the consumption of argentine anchovy, Engraulis anchoita, an underexploited species, did not reveal changes through time.},
  langid = {english},
  keywords = {Argentine Sea,Chondrichthyes,Fisheries,Fishing down,Marine ecosystem,Squalidae,Trophic ecology}
}

@article{Belleggia2012e,
  title = {The Feeding Ecology of {{Mustelus}} Schmitti in the Southwestern {{Atlantic}}: Geographic Variations and Dietary Shifts},
  shorttitle = {The Feeding Ecology of {{Mustelus}} Schmitti in the Southwestern {{Atlantic}}},
  author = {Belleggia, Mauro and Figueroa, Daniel E. and S{\'a}nchez, Felisa and Bremec, Claudia},
  year = {2012},
  month = sep,
  journal = {Environmental Biology of Fishes},
  volume = {95},
  number = {1},
  pages = {99--114},
  issn = {1573-5133},
  doi = {10.1007/s10641-011-9874-3},
  abstract = {The feeding ecology of Mustelus schmitti was studied based on analysis of stomach contents of 525 specimens collected from three research cruises carried out during October 2008 to March 2009 on the Argentinean continental shelf (34\textordmasculine 45{${'}$}S\textendash 41\textordmasculine S). Generalized Linear Models were used to test the effects of Geographic location, Total Length, Sex and Maturity stage on the diet. The most commonly occurring major prey groups differed between Geographic locations. In R\'io de la Plata (RLP) deep M. schmitti feed on Crustaceans, Fish and Polychaetes, whereas Crustaceans, Polychaetes and Fishes were the main prey in RLP coast, Rinc\'on (RIN) coast and RIN deep, respectively. Moreover, models showed dietary shift related to total length in each Geographic location. In coastal nursery areas Polychaetes decreased and Fishes increased in importance as M. schmitti grew in size; while Crustaceans increased in importance from small to medium size, and then decreased in the large specimens. Deeper areas also show dietary changes related to growth. The trophic level of M. schmitti showed lower values in coastal Geographic locations (3.48\textendash 3.53) than in deeper Geographic locations (3.56\textendash 3.77). The higher trophic role in deeper communities could be related to the size of M. schmitti which increases with depth. Finally, total vacuity index was 2.47 (the lowest in the literature) indicating an intense and continuous feeding activity of the species.},
  langid = {english},
  keywords = {Diet,Gatuzo,Models,Narrownose smoothhound,Nursery habitat,Ontogenetic changes},
  file = {/home/leonardo/Zotero/storage/X3X84M8U/Belleggia et al. - 2012 - The feeding ecology of Mustelus schmitti in the so.pdf}
}

@article{Belleggia2014b,
  title = {Spatio-Temporal and Ontogenetic Changes in the Diet of the {{Argentine}} Hake {{Merluccius}} Hubbsi},
  author = {Belleggia, Mauro and Figueroa, Daniel E. and Irusta, Gabriela and Bremec, Claudia},
  year = {2014},
  month = dec,
  journal = {Journal of the Marine Biological Association of the United Kingdom},
  volume = {94},
  number = {8},
  pages = {1701--1710},
  publisher = {{Cambridge University Press}},
  issn = {0025-3154, 1469-7769},
  doi = {10.1017/S0025315414000629},
  abstract = {The diet and the trophic level of the Argentine hake Merluccius hubbsi was investigated in the south-west Atlantic Ocean. Stomachs from 3405 specimens, ranging from 10 to 94~cm total length, collected on three research cruises carried out during winter 2011 and summer 2012, were examined at the laboratory. Dietary shifts related to sex, maturity stage, size, age, season, region and hour were examined using generalized linear models. The Argentine hake M. hubbsi fed mainly on zooplankton crustaceans (Themisto gaudichaudii, Euphausia lucens and Munida spp.), followed by fish and cephalopods. During summer, cephalopods (mainly Illex argentinus) were the main prey of M. hubbsi, indicating spatio-temporal changes in the diet. This work also revealed ontogenetic dietary changes associated with size, from zooplankton crustaceans during early life stages (E. lucens, T. gaudichaudii and Munida spp.) to fish and cephalopods. The trophic level of M. hubbsi ranged from 3.43 to 4.51 according to season and region. Cannibalism increased with predator length and contributed a maximum of 8.14\% to the diet of M. hubbsi during winter on the inner Patagonian continental shelf.},
  langid = {english},
  keywords = {feeding ecology,Merlucciidae,Patagonia,trophic level},
  file = {/home/leonardo/Zotero/storage/C9K6CZW8/40D118B0C2E4B6F67CD3F8A7E4785CE0.html}
}

@article{Belleggia2016b,
  ids = {Belleggia2016a},
  title = {Trophic Ecology of Yellownose Skate {{Zearaja}} Chilensis, a Top Predator in the South-Western {{Atlantic Ocean}}},
  author = {Belleggia, M. and Andrada, N. and Paglieri, S. and Cort{\'e}s, F. and Massa, A. M. and Figueroa, D. E. and Bremec, C.},
  year = {2016},
  journal = {Journal of Fish Biology},
  volume = {88},
  number = {3},
  pages = {1070--1087},
  issn = {1095-8649},
  doi = {10.1111/jfb.12878},
  abstract = {The diet and trophic level (TL) of the yellownose skate Zearaja chilensis in the south-western Atlantic Ocean (35\textdegree\textendash 54\textdegree{} S), and how these varied in relation to body size, sex, maturity stage, depth and region were determined by analysis of stomach contents. From 776 specimens analysed, 671 (86{$\cdot$}5\%) ranging from 180 to 1190 mm total length (LT) had prey in their stomachs. The diet was dominated by fishes, mainly the notothenioid Patagonotothen ramsayi and the Argentine hake Merluccius hubbsi. The consumption of fishes and crabs increased with increasing predator size, and these preys were more important in the north than in the south. Isopods and other crustaceans were consumed more in the south and their consumption decreased as the size of Z. chilensis increased. The TL of Z. chilensis increased with LT from 4{$\cdot$}29 to 4{$\cdot$}59 (mean 4{$\cdot$}53), confirming their ecological role as a top predator. The small and large size classes exhibited a low diet overlap and the highest spatial segregation, whereas medium and large specimens had higher co-occurrence and dietary overlap indices. A clear distinction in tooth shape was noted between sexes in adult specimens, with males having longer cusps. This sexual heterodonty may be related to reproductive behaviour, increasing the grasping ability of males during courtship, because there were no differences in diet between the sexes.},
  langid = {english},
  keywords = {dentition,diet,elasmobranchs,trophic level},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/jfb.12878},
  file = {/home/leonardo/Zotero/storage/D42FAHP4/Belleggia et al. - 2016 - Trophic ecology of yellownose skate Zearaja chilen.pdf;/home/leonardo/Zotero/storage/Y2GQJITS/Belleggia et al. - 2016 - Trophic ecology of yellownose skate Zearaja chilen.pdf;/home/leonardo/Zotero/storage/8B6QSUDM/jfb.html}
}

@article{Belleggia2017b,
  title = {Adaptation of Diet in a Changed Environment: {{Increased}} Consumption of Lobster Krill {{Munida}} Gregaria ({{Fabricius}}, 1793) by {{Argentine}} Hake},
  shorttitle = {Adaptation of Diet in a Changed Environment},
  author = {Belleggia, Mauro and Giberto, Diego and Bremec, Claudia},
  year = {2017},
  journal = {Marine Ecology},
  volume = {38},
  number = {4},
  pages = {e12445},
  issn = {1439-0485},
  doi = {10.1111/maec.12445},
  abstract = {A well-replicated decadal-term (2005\textendash 2014) stomach content data set was analysed in order to infer inter-annual fluctuations in the diet of the Argentine hake, Merluccius hubbsi, an opportunistic predator in the San Jorge Gulf (SJG) ecosystem in the Southwest Atlantic. Ten research cruises were carried out each year during January from 2005 to 2014. A total of 18,461 specimens of Me. hubbsi was analysed, of which 6,777 (36.71\%) contained food in their stomachs. The diet of Me. hubbsi changed markedly from 2011 onwards, with much greater consumption of the lobster krill Munida gregaria compared to the years before 2011. The frequency of occurrence (\%F) of Mu. gregaria in the stomach contents of Argentine hake increased from the year 2009 onwards, most noticeably since 2011, and mostly over the southern region of the SJG. The main predators of Mu. gregaria in the SJG are two species of teleost fish (pink cusk eel, Genypterus blacodes, Argentine seabass, Acanthistius brasilianus) and three Rajidae skates (Zearaja chilensis, Psammobatis spp. and Sympterygia bonapartii), which exhibited decreased catches in the years analysed. The increased consumption of Mu. gregaria by Me. hubbsi, coupled with decreased trends in abundance of the main predators of the lobster krill during the last decade, indicate that top-down trophic dynamic control occurs in the SJG ecosystem.},
  copyright = {\textcopyright{} 2017 Blackwell Verlag GmbH},
  langid = {english},
  keywords = {diet shift,Galatheidae,Merlucciidae,top-down,trophic ecology},
  file = {/home/leonardo/Zotero/storage/MHXDK6X6/Belleggia et al. - 2017 - Adaptation of diet in a changed environment Incre.pdf}
}

@article{Berrow1999b,
  title = {The Diet of White-Chinned Petrels {{Procellaria}} Aequinoctialis, {{Linnaeus}} 1758, in Years of Contrasting Prey Availability at {{South Georgia}}},
  author = {Berrow, Simon D. and Croxall, John P.},
  year = {1999},
  month = sep,
  journal = {Antarctic Science},
  volume = {11},
  number = {3},
  pages = {283--292},
  publisher = {{Cambridge University Press}},
  issn = {1365-2079, 0954-1020},
  doi = {10.1017/S0954102099000371},
  abstract = {The diet of breeding white-chinned petrels was studied during the summers of 1996 and 1998 at South Georgia. Krill abundance/availability was high throughout 1996 but apparently low at the beginning of the 1998 breeding season. The diet of white-chinned petrels was similar between years and consistent with previous studies. Krill Euphausia superba (41\textendash 42\% by weight) was the single most important prey item followed by fish (39\textendash 29\%) and squid (19\textendash 25\%). Meal mass was consistent (110 g in 1996, 119 g in 1998) between years but a significant decrease (46\%) in feeding frequency in 1998 (0.54 meals day-1 compared to 0.75 meals day-1 in 1996) resulted in 19\% less food delivered to chicks in 1998 than in 1996. Breeding success, however, was consistent between years at 44\% and similar to that recorded previously at Bird Island. This is in contrast to black-browed and grey-headed albatrosses, both of which experienced almost total breeding failure in 1998. It is suggested that their varied and versatile feeding methods, together with their greater diving ability, capacity to feed at night and extensive foraging range, help white-chinned petrels minimise the effects of krill shortage.},
  langid = {english},
  keywords = {diet,feeding frequency,inter-annual variation,krill,meal size,white-chinned petrel},
  file = {/home/leonardo/Zotero/storage/28JLE3M6/Berrow y Croxall - 1999 - The diet of white-chinned petrels Procellaria aequ.pdf;/home/leonardo/Zotero/storage/CWUAGCTY/79FF7286CC783A63D524F02194C21812.html}
}

@article{Berrow2000b,
  title = {Status of White-Chinned Petrels {{Procellaria}} Aequinoctialis {{Linnaeus}} 1758, at {{Bird Island}}, {{South Georgia}}},
  author = {Berrow, Simon D. and Croxall, John P. and Grant, Sharon D.},
  year = {2000},
  month = dec,
  journal = {Antarctic Science},
  volume = {12},
  number = {4},
  pages = {399--405},
  issn = {1365-2079, 0954-1020},
  doi = {10.1017/S0954102000000468},
  abstract = {The white-chinned petrel (Procellaria aequinoctialis) is an abundant, widespread petrel breeding in tussock grassland at sub-Antarctic islands. Over the last decade it has been killed in large numbers in temperate and sub-tropical longline fisheries. However no data are available on the global population status. We assessed the status of white-chinned petrels at Bird Island, South Georgia by comparing the distribution and density of occupied burrows in 1981 and 1998. In both surveys white-chinned petrel burrows occurred in one-quarter of the 460\textendash 477 36-m2 quadrats surveyed. The total number of burrows in each quadrat was consistent between each survey but we estimate an overall decrease of 28\% in those occupied (with considerable variation between sites). Concurrent data on breeding frequency and success showed that white-chinned petrels are essentially annual breeders at Bird Island; breeding success was consistent at around 44\%. Significant factors determining densities of occupied burrows were crown height and percent tussock cover (accounting for 77\% of variance). The former has decreased significantly, the latter increased significantly between 1981 and 1998 but there was no relationship between white-chinned petrel occupancy rate and habitat modification due to the presence of fur seals (Arctocephalus gazella). This suggests that any population decline is due to factors operating away from the breeding colony, such as those attributed to fishing.},
