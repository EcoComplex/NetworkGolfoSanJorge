@phdthesis{Balzi2005,
  title = {{Ecolog\'ia y biolog\'ia de la reproducci\'on de la centolla Lithodes santolla del golfo San Jorge.}},
  author = {Balzi, P.},
  year = {2005},
  abstract = {Los representantes de la familia Lithodidae son habitantes de aguas fr\'ias y templado fr\'ias tanto del hemisferio Norte como Sur, de aguas costeras y profundas, s\'olo dos son las especies explotadas comercialmente, la centolla com\'un (Lithodes santolla) y el centoll\'on o falsa centolla (P. granulosa). Ambas son capturadas en el estrecho de Magallanes y el archipi\'elago fueguino y s\'olo L. santolla es explotada en el golfo San Jorge, por embarcaciones con puerto en Santa Cruz y Chubut. El cap\'itulo 1 describe las caracter\'isticas medioambientales del Golfo San Jorge y las caracter\'isticas morfol\'ogicas de la centolla. Los aspectos reproductivos son abarcados por tres cap\'itulos, el cap\'itulo 2 abarca todo lo relacionado al ciclo reproductivo (desarrollo gonadal, fecundidad, madurez sexual, talla de madurez, morfolog\'ia de los aparatos reproductores femenino y masculino). El cap. 3 se dedica a la embiog\'enesis, y el 4 la Ecdisis (muda). El cap. 5 presenta los h\'abitos alimentarios, y el 6 la estructura de la poblaci\'on. Finaliza con recomendaciones para el manejo del recurso.},
  langid = {spanish},
  school = {UNIVERSIDAD NACIONAL DE LA PATAGONIA SAN JUAN BOSCO},
  annotation = {Accepted: 2018-09-19T13:47:24Z},
  file = {/home/leonardo/Zotero/storage/WRY3YACT/Balzi - 2005 - Ecología y biología de la reproducción de la cento.pdf;/home/leonardo/Zotero/storage/C3HX26KB/14435.html}
}

@article{Barbini2010b,
  title = {Feeding Habits of the {{Magellan}} Skate: Effects of Sex, Maturity Stage, and Body Size on Diet},
  shorttitle = {Feeding Habits of the {{Magellan}} Skate},
  author = {Barbini, Santiago A. and Scenna, Lorena B. and Figueroa, Daniel E. and Cousseau, Mar{\'i}a B. and {D{\'i}az de Astarloa}, Juan M.},
  year = {2010},
  month = mar,
  journal = {Hydrobiologia},
  volume = {641},
  number = {1},
  pages = {275--286},
  issn = {1573-5117},
  doi = {10.1007/s10750-010-0090-5},
  abstract = {The aim of this study was to evaluate the effects of sex, maturity stage, and body size on the diet of the Magellan skate, Bathyraja magellanica, in the Southwest Atlantic off Argentina, by examining stomach contents using a multiple hypothesis modeling approach. Relationships between the number of prey and sex, maturity stage, and total length (TL) were assessed by built generalized linear models (GLM). Furthermore, we tested whether there was a threshold size at which B. magellanica started or quit consuming a given prey. The overall diet of B. magellanica was mainly consisted of teleosts, followed by amphipods, isopods, and decapods. Ontogenetic diet shifts were independent of sex and maturity stage. However, discrete shifts in diet with TL were found, with individuals larger than 554 and 623~mm TL ceasing to consume amphipods and isopods, respectively. The consumption of teleosts progressively increased with increasing predator size. Likewise, ontogenetic shifts in foraging behavior were also observed with smaller individuals showing specialization on amphipods with larger specimens consuming teleosts. These results confirm that ontogenetic shifts in diet of B. magellanica are more a function of predator size rather than any other life-history traits. We propose that these food shifts are probably related to morphological limitations and abilities associated with feeding habits of skate, so when specimens of B.~magellanica reach an optimum body size, they may have access to higher quality trophic resources. Our results suggest that evaluating the importance of life-history stages on the feeding habits of a species is essential for understanding how that species exploits food resources, which, in turn, is an important factor in developing a suitable plan of marine ecosystem conservation.},
  langid = {english}
}

@article{Barbini2011b,
  title = {Feeding Habits of a Large Endangered Skate from the South-West {{Atlantic}}: The Spotback Skate, {{Atlantoraja}} Castelnaui},
  shorttitle = {Feeding Habits of a Large Endangered Skate from the South-West {{Atlantic}}},
  author = {Barbini, Santiago A. and Lucifora, Luis O. and Barbini, Santiago A. and Lucifora, Luis O.},
  year = {2011},
  month = nov,
  journal = {Marine and Freshwater Research},
  volume = {63},
  number = {2},
  pages = {180--188},
  publisher = {{CSIRO PUBLISHING}},
  issn = {1448-6059, 1448-6059},
  doi = {10.1071/MF11170},
  abstract = {Elasmobranch predation has important effects on marine ecosystems. Identifying the main correlates of the feeding habits of skates is of paramount importance for determining their ecological role. We tested the hypotheses that the diet of the spotback skate, Atlantoraja castelnaui, off Uruguay and northern Argentina, changes with increasing body size, between seasons and regions and that prey size increased with predator's size using a multiple-hypothesis modelling approach. A. castelnaui preyed mainly on teleosts, followed by cephalopods, elasmobranchs and decapods. Small individuals of A. castelnaui consumed decapods and large individuals ate elasmobranchs and cephalopods. The consumption of teleosts was constant along the ontogeny but differed between seasons; more demersal-benthic teleosts were consumed in the cold season, whereas more benthic teleosts were eaten in the warm season. Also, A. castelnaui consumed more cephalopods in the warm season than in the cold season. Benthic teleosts were consumed more in the south region, whereas decapods were eaten more in the north region. A. castelnaui is able to consume larger teleosts as it grows. We conclude that A. castelnaui is a versatile, mainly piscivorous, consumer that shifts its diet with increasing body size and in response to seasonal and regional changes in prey abundance or distribution.},
  langid = {english},
  file = {/home/leonardo/Zotero/storage/CVLK4THC/MF11170.html}
}

@article{Barbini2016b,
  title = {Diet Composition and Feeding Habits of the Eyespot Skate, {{{\emph{Atlantoraja}}}}{\emph{ Cyclophora}} ({{Elasmobranchii}}: {{Arhynchobatidae}}), off {{Uruguay}} and Northern {{Argentina}}},
  shorttitle = {Diet Composition and Feeding Habits of the Eyespot Skate, {{{\emph{Atlantoraja}}}}{\emph{ Cyclophora}} ({{Elasmobranchii}}},
  author = {Barbini, Santiago A. and Lucifora, Luis O.},
  year = {2016},
  month = sep,
  journal = {Neotropical Ichthyology},
  volume = {14},
  publisher = {{Sociedade Brasileira de Ictiologia}},
  issn = {1679-6225, 1982-0224},
  doi = {10.1590/1982-0224-20160032},
  abstract = {ABSTRACT The eyespot skate, Atlantoraja cyclophora, is an endemic species from the southwestern Atlantic, occurring from Rio de Janeiro, Brazil, to northern Patagonia, Argentina. The feeding habits of this species, from off Uruguay and north Argentina, were evaluated using a multiple hypothesis modelling approach. In general, the diet was composed mainly of decapod crustaceans, followed by teleost fishes. Molluscs, mysidaceans, amphipods, isopods, lancelets and elasmobranchs were consumed in lower proportion. The consumption of shrimps drecreased with increasing body size of A. cyclophora. On the other hand, the consumption of teleosts increased with body size. Mature individuals preyed more heavily on crabs than immature individuals. Teleosts were consumed more in the south region (34\textordmasculine{} - 38\textordmasculine S) and crabs in the north region (38\textordmasculine{} - 41\textordmasculine S). Shrimps were eaten more in the warm season than in the cold season. Prey size increased with increasing body size of A. cyclophora , but large individuals also consumed small teleosts and crabs. Atlantoraja cyclophora has demersal-benthic feeding habits, shifts its diet with increasing body size and in response to seasonal and regional changes in prey availability and distribution.},
  langid = {english},
  keywords = {Chondrichthyes,Ontogenetic shifts,Predation,Southwest Atlantic},
  file = {/home/leonardo/Zotero/storage/ESLBQLJS/Barbini y Lucifora - 2016 - Diet composition and feeding habits of the eyespot.pdf;/home/leonardo/Zotero/storage/KU8KADXL/YyqtM36TjRydpbf89Mz535f.html}
}

