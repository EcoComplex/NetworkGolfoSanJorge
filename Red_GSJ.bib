@article{Mariano-Jelicich2011,
  title = {The Diet of the {{South American Tern}}: The {{Argentine Anchovy}} as Key Prey in the Non-Breeding Season},
  shorttitle = {The Diet of the {{South American Tern}}},
  author = {Mariano-Jelicich, Rocío and Rodriguez, María P. Silva and Copello, Sofía and Pon, Juan P. Seco and Berón, María P. and Mauco, Laura and Ghys, Mariela I. and Favero, Marco},
