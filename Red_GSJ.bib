@article{Crespo1997,
  title = {Direct and {{Indirect Effects}} of the {{Highseas Fisheries}} on the {{Marine Mammal Populations}} in the {{Northern}} and {{Central Patagonian Coast}}},
  author = {Crespo, E A and Pedraza, S N and Dans, S L and Koen Alonso, M and Reyes, L M and García, N A and Coscarella, M and Schiavini, A C M},
  date = {1997-12},
  journaltitle = {Journal of Northwest Atlantic Fishery Science},
  shortjournal = {J. Northw. Atl. Fish. Sci.},
  volume = {22},
