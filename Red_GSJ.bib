@article{Pauly1998,
  title = {Fishing {{Down Marine Food Webs}}},
  author = {Pauly, Daniel and Christensen, Villy and Dalsgaard, Johanne and Froese, Rainer and Torres, Francisco},
  date = {1998-02-06},
  journaltitle = {Science},
  volume = {279},
