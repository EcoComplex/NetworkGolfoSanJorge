@book{Brunetti1999,
  title = {Calamares de importancia comercial en Argentina. Biología, distribución, pesquerías y muestreo biológico},
  author = {Brunetti, N. E. and Ivanovic, M. L. and Sakai, M.},
  date = {1999},
  url = {https://aquadocs.org/handle/1834/2336},
  urldate = {2022-04-13},
  abstract = {An overview of the available information on the commercially important squid species distributed in the Argentine Sea and adjacent zones is given. It includes the most representative characteristics to identify the species and general information on their biology, distribution and fisheries. A biological sampling designis presented, with particular emphasis on the identification of sexual maturity stages. The world market of the squid resources has considerably risen in the last decades and one of the most important fishery zones is placed in the Southwest Atlantic region (Area 41, FAO), where squid represent 25. of the total landings. Illex argentinus, sustains the second fishery in volume, with landings reaching 612.000 tonnes in 1999. Other species, such as Loligo sanpaulensis, Loligo gahi and Martialia hyadesi, are supporting small domestic fisheries. Also included is the current knowledge on Ommastrephes bartrami and Thysanoteuthis rhombus, cosmopolitan species that sustain important fisheries in other regions, and could be fishery exploitably alternatives here. A special case is that of Moroteuthis ingens, a worldwide particularly abundant species, but whose exploitation has been not developed yet. Finally, the giant squid (Architeuthis sp.)is included, as it started to appear frequently in this region during the last years and its knowledge is of high scientific value.},
  isbn = {978-987-96244-2-5},
  langid = {spanish},
  annotation = {Accepted: 2008-05-06T23:38:40Z},
  file = {/home/leonardo/Zotero/storage/MP53E94A/Brunetti et al. - 1999 - Calamares de importancia comercial en Argentina. B.pdf;/home/leonardo/Zotero/storage/YNE6S5A5/2336.html}
}

@article{Bruno2000,
  title = {Contribution of Polychaetous Annelids to the Diet of {{Cheilodactylus}} Bergi ({{Pisces}}, {{Cheilodactylidae}}) in {{Argentina}}},
  author = {Bruno, Claudia and Cousseau, María Berta and Bremec, Claudia Silvia},
  date = {2000-07-01},
  journaltitle = {Bulletin of Marine Science},
  shortjournal = {Bulletin of Marine Science},
  volume = {67},
  number = {1},
  pages = {277--286},
  abstract = {The hawkfish (castañeta) Cheilodactylus bergi Norman, 1937 is distributed in the Atlantic Ocean, from southern Brazil (22°S) to the Gulf of San Jorge in Argentina (47°S), between 50 and 150 m depth, and constitutes a fishing resource of commercial value. Previous studies indicated that polychaetes constitute its main food. The contribution of different families of worms to the diet of hawkfish and the relationships between those taxa and the benthic fauna of the feeding grounds are presented in this paper. A total of 522 specimens of fish was measured, weighed and the gut contents analyzed. Polychaete families, mainly Onuphidae, Lumbrineridae and Eunicidae, were the most important feeding items in specimens from 28 to 51 cm total length, both in percent of frequence (\%F) and percent of weight (\%W). Occasional presence of Aphroditidae, Arabellidae, Palmyridae, Lysaretidae and Dorvilleidae was recorded. Bibliographic data on faunistic composition of feeding grounds indicate that polychaetes constitute between 12 to 21\% of number of taxa of macroinvertebrates. C. bergi feed upon 25–38\% of species of polychaetes present in the area. It is concluded that C. bergi feed on polychaetes which are mandibulated, implying mobility over the benthic surface.}
}

@article{Cassemiro2003,
  title = {Diet and Trophic Ecomorphology of the Silverside, {{Odontesthes}} Bonariensis, of the {{Salto Caxias}} Reservoir, Rio {{Iguaçu}}, {{Paraná}}, {{Brazil}}},
  author = {Cassemiro, Fernanda Aparecida da Silva and Hahn, Norma Segatti and Rangel, Thiago Fernando Lopes Valle de Britto},
  date = {2003-12},
  journaltitle = {Neotropical Ichthyology},
  shortjournal = {Neotrop. ichthyol.},
  volume = {1},
  pages = {127--131},
  publisher = {{Sociedade Brasileira de Ictiologia}},
  issn = {1679-6225, 1982-0224},
  doi = {10.1590/S1679-62252003000200007},
  url = {http://www.scielo.br/j/ni/a/6yRggqtQ9PgP4sGS6xdyF4c/abstract/?lang=en},
  urldate = {2022-04-13},
  abstract = {This study aims to analyze the diet and trophic ecomorphology of Odontesthes bonariensis, relating these features with species' abundance in the natural environment and in the Salto Caxias reservoir (at Iguaçu river) after the reservoir's formation. The samples were carried out before (from March 1997 to February 1998- pre) and after (from October 1998 to February 2001 - post 1 and 2) the damming. The stomach contents of 218 individuals were analyzed by occurrence and volumetric methods. O. bonariensis' diet is based on insects, scales, plants and detritus. Microcrustaceans (Cladocera and Copepoda) were the main items, allowing the characterization of the species as zooplanktivore. The species has a superior position and protractible mouth, with small teeth, creating a serrated pattern, uniformly lined side by side. There are many gill rakers, that are long and close together (typical of filtering fishes); the stomach is ill defined and the intestine is short. There was a gradual increase of O. bonariensis' abundance during the period of field study, mainly after the second year of damming. This fact seems to be closely related with the high abundance and availability of zooplankton in the dammed environment, and also with the ability of the species to exploit this resource.},
  langid = {english},
  keywords = {Atherinidae,feeding,freshwater,morphology,planktivory},
  file = {/home/leonardo/Zotero/storage/WR5HK2SJ/Cassemiro et al. - 2003 - Diet and trophic ecomorphology of the silverside, .pdf;/home/leonardo/Zotero/storage/46SEFPMZ/abstract.html}
}

