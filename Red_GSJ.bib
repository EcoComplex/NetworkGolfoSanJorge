@article{Link2002,
  title = {Trophic Ecology of {{Atlantic}} Cod {{Gadus}} Morhua on the Northeast {{US}} Continental Shelf},
  author = {Link, Jason S. and Garrison, Lance P.},
  date = {2002-02-13},
  journaltitle = {Marine Ecology Progress Series},
